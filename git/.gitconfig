[user]
  name = Phillip Boksz
  email = 
  token = 
[color]
  diff = auto
  status = auto
  branch = auto
  ui = true
[alias]
  ka = !gitk --all
  reflogr = reflog --date=relativeÂ·
  pom = push origin master
  lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
  serve = !git daemon --reuseaddr --verbose  --base-path=. --export-all ./.git
  st = status -sb
  co = checkout
  cm = commit -m
  ca = commit --amend
  di = diff
  ds = diff --staged
  pr = pull --rebase
  rc = rebase --continue
  sh = stash
  sp = stash pop
  un = reset --soft HEAD^
[core]
  editor = vim
  excludesfile = ~/.gitignore_global
[merge]
  tool = vimdiff
[push]
  default = tracking
[help]
  autocorrect = 20
